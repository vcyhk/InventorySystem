<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="setScene" prefHeight="400.0" prefWidth="600.0" stylesheets="@/css/changeProPicCSS.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="setting.settingChangeProPic.ChangeProPicController">


    <HBox alignment="CENTER_LEFT" prefHeight="64.0" prefWidth="686.0">
        <children>
            <Label prefHeight="17.0" prefWidth="78.0" text="File Name :">
                <font>
                    <Font name="Corbel Bold" size="14.0" />
                </font>
                <HBox.margin>
                    <Insets left="25.0" />
                </HBox.margin>
            </Label>
            <Label fx:id="filename" prefHeight="18.0" prefWidth="309.0">
                <font>
                    <Font name="Corbel Bold" size="14.0" />
                </font>
                <HBox.margin>
                    <Insets left="20.0" right="20.0" />
                </HBox.margin>
            </Label>
            <Button fx:id="selectBtn" mnemonicParsing="false" onAction="#selectFile" prefHeight="30.0" prefWidth="111.0" text="Choose File" textFill="#fafafa">
                <font>
                    <Font name="Calibri Bold" size="16.0" />
                </font>
            </Button>
        </children>
        <padding>
            <Insets top="20.0" />
        </padding>
    </HBox>
   <Pane prefHeight="335.0" prefWidth="600.0">
      <children>
         <ImageView fx:id="icon" fitHeight="232.0" fitWidth="290.0" layoutX="155.0" layoutY="35.0" pickOnBounds="true" preserveRatio="true" />
      </children>
   </Pane>
   <HBox prefHeight="57.0" prefWidth="600.0">
       <children>
           <VBox alignment="TOP_CENTER" prefHeight="35.0" prefWidth="147.0">
               <children>
                   <Button fx:id="submitBtn" mnemonicParsing="false" onAction="#submitPhoto" prefHeight="33.0" prefWidth="117.0" text="Submit" textFill="WHITE">
                       <VBox.margin>
                           <Insets bottom="20.0" right="40.0" />
                       </VBox.margin>
                       <font>
                           <Font name="Calibri Bold" size="17.0" />
                       </font>
                   </Button>
               </children>
            <HBox.margin>
               <Insets left="446.0" />
            </HBox.margin>
           </VBox>
       </children>
       <VBox.margin>
           <Insets left="5.0" top="10.0" />
       </VBox.margin>
    </HBox>

</VBox>
